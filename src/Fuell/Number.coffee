
exports.Number = Number

exports.sum = 
sum = (y, x) -> x + y

exports.difference = 
difference = (y, x) -> x - y

exports.product = 
product = (y, x) -> x * y

exports.quotient = 
quotient = (y, x) -> x / y

exports.even = 
even = (x) -> x % 2 == 0

exports.odd = 
odd = (x) -> x % 2 == 1

exports.times = 
times = (y, x) -> y for i in [0...x]


